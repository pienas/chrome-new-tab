{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Naujas skirtukas",
  "author": "Karolis Urbaitis",
  "version": "1.1.4",
  "description": "Custom skirtukai.",
  "permissions": [
    "storage",
    "topSites",
    "notifications",
    "alarms",
    "contextMenus",
    "unlimitedStorage",
    "clipboardRead",
    "http://suggestqueries.google.com/"
  ],
  "manifest_version": 3,
  "short_name": "Naujas skirtukas",
  "host_permissions": ["<all_urls>", "*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Atidaryti naują skirtuką"
  }
}
